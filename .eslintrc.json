{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "angular": false,
        "Papa": false,
        "svgPanZoom": false,
        "io": false,
        "diffsync": false,
        "wysihtml5": false,
        "wysihtml5ParserRules": false,
        "GUID": false,
        "PDFJS": false,
        "CodeMirror": false,
        "Raven": false,
        "require": false,
        "module": false
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-return-assign": [
          2,
          "always"
        ],
        "no-undef": 2,
        "keyword-spacing": 2,
        "block-spacing": [
          1,
          "always"
        ],
        "eqeqeq": 2,
        "dot-notation": 2,
        "no-extra-bind": 2,
        "comma-dangle": [
          1,
          "never"
        ],
        "strict": 2,
        "array-bracket-spacing": [
          2,
          "never"
        ],
        "block-scoped-var": 2,
        "consistent-return": 2,
        "comma-spacing": [
          2,
          {
            "after": true
          }
        ],
        "space-before-blocks": 2,
        "no-multi-spaces": 2,
        "curly": 2,
        "no-alert": 2,
        "guard-for-in": 2,
        "key-spacing": 2,
        "no-caller": 2,
        "brace-style": [
          2,
          "1tbs"
        ],
        "no-unused-vars": 1,
        "no-extra-semi": 2,
        "comma-style": [
          2,
          "last"
        ],
        "dot-location": [
          2,
          "property"
        ],
        "no-eq-null": 2,
        "default-case": 2,
        "no-fallthrough": 2,
        "no-console": 0,
        "no-use-before-define": 2
        }
}